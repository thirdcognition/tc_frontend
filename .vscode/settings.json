{
    "eslint.workingDirectories": [
        {
            "directory": "lib_ts"
        }
    ],
    "eslint.useFlatConfig": true,
    "prettier.configPath": ".prettierrc",
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "prettier.trailingComma": "none",
    "vs-code-prettier-eslint.prettierLast": true,
    "editor.tabSize": 4,
    "[typescript]": {
        "editor.tabSize": 4
    },
    "[typescriptreact]": {
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.tabSize": 4
    },
    "[javascriptreact]": {
        "editor.tabSize": 4
    }
}
